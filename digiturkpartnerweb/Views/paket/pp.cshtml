﻿@model digiturkveri.paketkampanya
@{
    ViewBag.Title = Model.padi;
}


<div class="modal fade formModal" role="dialog" aria-labelledby="formModal" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content">
            <img class="btn-close" src="~/dosyalar/Assets/images/base/close.svg" data-dismiss="modal">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="title white-color gilroy-eb f-30 fw-Bold">Sizi Arayalım</div>
                        <p class="white-color f-16 gilroy-l mt-15 subtext">Kampanyalarımız ve tüm gelişmeler hakkında detaylı bilgi almak için formu doldurun, hemen sizi arayalım.</p>
                    </div>
                </div>
                <form action="/tumpaketler/basvuru" method="post" class="form-inline">
                    <div class="form-group mb-2 first-field">
                        <label class="f-15 gilroy-eb white-color input-label" for="name">AD SOYAD</label>
                        <input type="text" id="kelime" name="kelime" class="form-control input-md gilroy-l br-4" placeholder="Ad ve soyadınızı girin" autocomplete="off" required="required">
                    </div>
                    <input type="hidden" name="numara" id="numara" value="6" />
                    <input type="hidden" name="basipadress" id="basipadress" value="@Request.UserHostAddress" />
                    <div class="form-group mx-sm-3 mb-2 second-field">
                        <label class="f-15 gilroy-eb white-color input-label" for="phone">TELEFON</label>
                        <input type="text" id="tele" name="tele" class="form-control input-md gilroy-l br-4 phone-mask" placeholder="(5__) ___ __ __" autocomplete="off" required="required">
                    </div>
                    <label class="f-15 gilroy-eb white-color input-label" for="phone" type="text" id="numara" name="numara" hidden>6</label>
                    <a class="white-color gilroy-l fw-Bold f-18 text-center d-block br-30 margin-auto btn-submit mb-2" href="javascript:;" onclick="SendUtmParameter(this)">Gönder</a>
                    <div class="form-kvkk">
                        <p>Potansiyel Müşteri Aydınlatma metni için <a target="_blank" href="Assets/pdf/digiturk_kvkk.pdf">tıklayın</a>.</p>
                        <div class="pretty p-svg p-curve">
                            <input type="checkbox" id="isbullet" name="isbullet" />
                            <div class="state p-success">
                                <!-- svg path -->
                                <svg class="svg svg-icon" viewBox="0 0 20 20">
                                    <path d="M7.629,14.566c0.125,0.125,0.291,0.188,0.456,0.188c0.164,0,0.329-0.062,0.456-0.188l8.219-8.221c0.252-0.252,0.252-0.659,0-0.911c-0.252-0.252-0.659-0.252-0.911,0l-7.764,7.763L4.152,9.267c-0.252-0.251-0.66-0.251-0.911,0c-0.252,0.252-0.252,0.66,0,0.911L7.629,14.566z" style="stroke: white;fill:white;"></path>
                                </svg>
                                <label>Digiturk (Digital Platform Teknoloji Hizmetleri A.Ş.) tarafından  formda belirtmiş olduğum iletişim adresi üzerinden tarafıma ticari elektronik ileti gönderilmesini kabul ediyorum.</label>
                            </div>
                        </div>
                    </div>
                </form>

                <hr />
                <div class="row">
                    <div class="col-12">
                        <p class="white-color f-16 gilroy-l">Ayrıca tüm sorularınız için bize aşağıdaki telefon numarasından ulaşabilirsiniz.</p>
                    </div>
                    <div class="col-12">
                        <div class="media mt-35">
                            <div class="image-area position-relative mr-3">
                                <div class="call-animation-outer"></div>
                                <div class="call-animation-outer-fill"></div>
                                <div class="call-animation">
                                    <i class="form-phone-ico"></i>
                                </div>
                            </div>
                            <div class="media-body">
                                <h5 class="mt-0 f-15 white-color gilroy-l fw-Bold">BİZE ULAŞIN</h5>
                                <a href="tel:@Html.Partial("_numara", digiturkpartnerweb.Models.Data.siteayar)" class="white-color gilroy-eb f-30 fw-Bold number mt-11 d-block">@Html.Partial("_numara", digiturkpartnerweb.Models.Data.siteayar)</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



