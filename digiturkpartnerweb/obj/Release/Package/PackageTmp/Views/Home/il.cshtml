@model IEnumerable<digiturkveri.paketkampanya>
@{
    ViewBag.Title = ViewBag.isim;
}



<section class="slider-content d-none d-lg-block">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="sliderWrap">
                    <div class="sliderArea" style="position:absolute; top:0px; z-index:0; width:100%; height:555px;">
                        <div class="banner" data-startHeight="555">
                            <ul>
                                <!-- first item -->
                                @Html.Partial("_spor", digiturkpartnerweb.Models.Data.spor)
                                <!-- second item -->
                                @Html.Partial("_dizi", digiturkpartnerweb.Models.Data.taraftar)
                                <!-- third item -->
                                @Html.Partial("_film", digiturkpartnerweb.Models.Data.dizi)
                            </ul>
                        </div>
                        <div class="campaign-area">
                            <div class="left-radius"></div>
                            <div class="right-radius"></div>
                            <div class="all-campaign">
                                <div class="campaign-outer">
                                    @Html.Partial("_spor1", digiturkpartnerweb.Models.Data.spor)
                                </div>
                                @Html.Partial("_dizi1", digiturkpartnerweb.Models.Data.taraftar)
                                @Html.Partial("_film1", digiturkpartnerweb.Models.Data.dizi)
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="mobile-slider-content d-block d-lg-none">
